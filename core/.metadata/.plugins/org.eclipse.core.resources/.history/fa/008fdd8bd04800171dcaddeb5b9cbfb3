package parser;

import java.util.List;

public class OrdChoice implements Parser {
	public final Parser p1;
	public final Parser p2;
	
	public OrdChoice(Parser p1, Parser p2) {
		this.p1 = p1;
		this.p2 = p2;
	}

	@Override
	public List<Token> parse(Error err, List<Token> ent) {
		try {
			return p1.parse(err, ent);
		} catch(RuntimeException f) {
			return p2.parse(err, ent);
		}
	}

}
