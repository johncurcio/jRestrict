String inijscript = " resetEvents -- comments about stuff";
peg.peg.run(ScriptParser.machine, inijscript);



String jscript = ""  
	     + " files { Main.java; }\n";

peg.Pair<mast.Machine, java.util.List<String>> res = peg.peg.run(ScriptParser.files, jscript);
mast.CollectVisitor cv = new mast.CollectVisitor(res.y);
mast.BindingVisitor bv = new mast.BindingVisitor(res.y);
res.x.visit(cv, null);
res.x.visit(bv, null);
return res;
