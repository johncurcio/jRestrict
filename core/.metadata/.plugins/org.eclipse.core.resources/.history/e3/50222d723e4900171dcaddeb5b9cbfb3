String jscript = ""  
	     + " files { Main.java;} ";

peg.Pair<mast.Files, java.util.List<String>> res = peg.peg.run(ScriptParser.files, jscript);
mast.CollectVisitor cv = new mast.CollectVisitor(res.y);
mast.BindingVisitor bv = new mast.BindingVisitor(res.y);
res.x.visit(cv, null);
res.x.visit(bv, null);
return res;n