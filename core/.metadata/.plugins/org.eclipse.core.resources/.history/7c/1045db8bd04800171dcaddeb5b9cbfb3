package parser;

import java.util.List;
import java.util.function.Supplier;

public class LazyParser implements Parser {
	Supplier<Parser> sp;
	
	public LazyParser(Supplier<Parser> sp) {
		this.sp = sp;
	}
	
	@Override
	public List<Token> parse(Error err, List<Token> ent) {
		return sp.get().parse(err, ent);
	}

}